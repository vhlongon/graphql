{
  "name": "graphql",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/vhlongon/graphql.git",
  "author": "Victor Hugo Longon <victor.longon@yahoo.com.br>",
  "license": "MIT",
  "scripts": {
    "build": "babel client --out-dir dist/client --ignore **/__tests__/**",
    "build:server": "babel server --out-dir dist/server --ignore **/__tests__/**",
    "serve": "yarn run build:server && nodemon server/index.js",
    "dev": "NODE_ENV=development webpack-dev-server",
    "start": "yarn run serve & yarn run dev",
    "lint": "eslint --ext .jsx,.js client",
    "test": "jest",
    "test:watch": "yarn run lint && jest --watch"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "browser-sync": "^2.18.13",
    "browser-sync-webpack-plugin": "^1.2.0",
    "enzyme": "^2.9.1",
    "eslint": "^4.4.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-prettier": "2.3.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.2.0",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^20.0.4",
    "nodemon": "^1.11.0",
    "prettier": "1.5.3",
    "react-test-renderer": "^15.6.1",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1"
  },
  "peerDependencies": {
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "dependencies": {
    "express": "^4.15.4",
    "express-graphql": "^0.6.7",
    "graphql": "^0.10.5",
    "lodash": "^4.17.4",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "es2017",
      "react"
    ],
    "plugins": [
      "transform-object-rest-spread",
      "transform-class-properties"
    ]
  }
}
