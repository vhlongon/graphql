    "node-fetch": "^1.7.2",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-apollo": "^1.4.15",
    "react-dom": "^15.6.1",
    "react-emotion": "^7.2.0",
    "react-router-dom": "^4.2.2",
    "recompose": "^0.25.0",
    "webpack": "^3.5.5",
    "webpack-dev-middleware": "^1.12.0"
  },
  "babel": {
    "plugins": [
      "emotion/babel",
      "transform-object-rest-spread",
      "transform-class-properties"
    ],
    "presets": [
      "es2015",
      "es2017",
      "react"
    ]
